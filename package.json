{
  "name": "vue-reload",
  "version": "1.2.1",
  "description": "make reload vue component easier",
  "type": "module",
  "sideEffects": false,
  "main": "dist/vue-reload.js",
  "module": "dist/vue-reload.js",
  "unpkg": "dist/vue-reload.js",
  "jsdelivr": "dist/vue-reload.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-reload.js"
    },
    "./compat": {
      "types": "./dist/index.compat.d.ts",
      "import": "./dist/vue-reload.compat.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "vite build",
    "prepublishOnly": "run-s build"
  },
  "keywords": [
    "vue",
    "reload",
    "refresh",
    "component",
    "vueuse",
    "hook"
  ],
  "author": "Xu Yiming",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuyimingwork/vue-reload.git"
  },
  "bugs": {
    "url": "https://github.com/xuyimingwork/vue-reload/issues"
  },
  "homepage": "https://github.com/xuyimingwork/vue-reload#readme",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.11",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "vue": "^2.3.0 || >=3.0.0"
  }
}
